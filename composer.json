{
    "name": "ixudra/authentication",
    "description": "Custom PHP Authentication library for the Laravel 5 framework - developed by Ixudra",
    "version": "1.0.0",
    "keywords": ["Ixudra", "Laravel", "Authentication"],
    "homepage": "http://ixudra.be",
    "license": "MIT",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@gmail.com"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "ixudra/core": "5.*",
        "ixudra/translation": "5.*",
        "laracasts/presenter": "0.2.*"
    },

    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "mockery/mockery": "~0.9"
    },

    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Ixudra\\Authentication\\": "src/"
        }
    },

    "minimum-stability": "stable"
}
